{% extends 'administration/index.html.twig' %}

{% block title %}Crear habilidad{% endblock %}

{% block styles %}
    {{ parent() }}

    <link rel="stylesheet" type="text/css" href="/assets/styles/form.css" />
{% endblock %}

{% block main %}
    {{ form_start(form) }}
        {% for field in form %}
            <div>
                {{ form_label(field) }}
                {{ form_widget(field) }}
                {{ form_errors(field) }}
            </div>
        {% endfor %}

        <input type="submit" value="Crear">
    {{ form_end(form) }}
{% endblock %}